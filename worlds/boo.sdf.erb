<!-- The base of operations -->
<model name="boo">
  <pose>0 0 0.5 0 0 0</pose>
  <static>true</static>
  <link name="link">
     <kinematic>true</kinematic>
     <collision name="collision">
       <geometry>
         <box>
           <size>1 1 1</size>
         </box>
       </geometry>
       <surface>
         <friction>
           <ode>
             <mu>0.0</mu>
             <mu2>0.0</mu2>
           </ode>
         </friction>
       </surface>
     </collision>
     <visual name="visual">
       <geometry>
         <box>
           <size>1 1 1</size>
         </box>
       </geometry>
     </visual>
   </link>

   <plugin name="boo" filename="libBooControllerPlugin.so">
     <lost_person_model>lost_person</lost_person_model>
     <!-- Do not change this address -->
     <address>3</address>
     <type>boo</type>
     <%=search_area%>
   </plugin>
</model>
